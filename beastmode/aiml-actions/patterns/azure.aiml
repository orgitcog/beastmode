<category>
  <pattern>CREATE TENANT *</pattern>
  <template>
    <think><set name="tenant_name"><star/></set></think>
    <action workflow="azure-create-tenant" inputs='{"name": "<star/>"}'/>
    Creating Azure AD tenant: <star/>
    
    This will provision a new Azure AD tenant with default configuration.
    Note: Requires Azure subscription-level permissions.
  </template>
</category>

<category>
  <pattern>CREATE * TENANTS</pattern>
  <template>
    <think><set name="tenant_count"><star/></set></think>
    <confirm>
      You're about to create <star/> Azure AD tenants.
      This is a significant operation. Proceed?
    </confirm>
    <action workflow="azure-bulk-tenants" inputs='{"count": "<star/>"}'/>
  </template>
</category>

<category>
  <pattern>LIST TENANTS</pattern>
  <template>
    <action workflow="azure-list-tenants" inputs='{}'/>
    Fetching list of Azure AD tenants...
  </template>
</category>

