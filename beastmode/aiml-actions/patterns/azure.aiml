<!-- MASS PROVISIONING (GOD MODE) -->
<!-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê -->

<category>
  <pattern>DEPLOY * TENANTS WITH * ORGS AND * USERS</pattern>
  <template>
    <think>
      <set name="tenants"><star index="1"/></set>
      <set name="orgs"><star index="2"/></set>
      <set name="users"><star index="3"/></set>
    </think>
    <confirm>
      üöÄ MASS PROVISIONING REQUEST
      
      This will create:
      ‚Ä¢ <star index="1"/> Azure AD tenants
      ‚Ä¢ <star index="2"/> organizations per tenant
      ‚Ä¢ <star index="3"/> users per organization
      
      Total resources:
      ‚Ä¢ <compute><star index="1"/> * <star index="2"/></compute> organizations
      ‚Ä¢ <compute><star index="1"/> * <star index="2"/> * <star index="3"/></compute> users
      
      ‚ö†Ô∏è This is a significant operation. Proceed?
    </confirm>
    <action workflow="mass-provision" inputs='{
      "tenants": "<star index="1"/>",
      "orgs": "<star index="2"/>",
      "users": "<star index="3"/>"
    }'/>
    Initiating mass provisioning...
  </template>
</category>

<category>
  <pattern>PROVISION * TENANTS * ORGS * USERS</pattern>
  <template>
    <srai>DEPLOY <star index="1"/> TENANTS WITH <star index="2"/> ORGS AND <star index="3"/> USERS</srai>
  </template>
</category>

<category>
  <pattern>BEAST MODE * * *</pattern>
  <template>
    <srai>DEPLOY <star index="1"/> TENANTS WITH <star index="2"/> ORGS AND <star index="3"/> USERS</srai>
  </template>
</category>

<category>
  <pattern>GOD MODE * * *</pattern>
  <template>
    <srai>DEPLOY <star index="1"/> TENANTS WITH <star index="2"/> ORGS AND <star index="3"/> USERS</srai>
  </template>
</category>

