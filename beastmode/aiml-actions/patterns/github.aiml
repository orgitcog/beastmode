<category>
  <pattern>MERGE PR * IN *</pattern>
  <template>
    <confirm>
      Merge pull request #<star index="1"/> in <star index="2"/>?
    </confirm>
    <action workflow="github-merge-pr" inputs='{"pr": "<star index="1"/>", "repo": "<star index="2"/>"}'/>
  </template>
</category>

<!-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê -->
<!-- DEPLOYMENT OPERATIONS -->
<!-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê -->

<category>
  <pattern>DEPLOY *</pattern>
  <template>
    <choice id="environment">
      Where would you like to deploy <star/>?
      <option value="production">Production</option>
      <option value="staging">Staging</option>
      <option value="development">Development</option>
    </choice>
  </template>
</category>

<category>
  <pattern>DEPLOY * TO PRODUCTION</pattern>
  <template>
    <confirm>
      ‚ö†Ô∏è PRODUCTION DEPLOYMENT
      
      You are about to deploy <star/> to PRODUCTION.
      This will affect all users.
      
      Proceed?
    </confirm>
    <action workflow="deploy-production" inputs='{"app": "<star/>"}'/>
  </template>
</category>

<category>
  <pattern>DEPLOY * TO STAGING</pattern>
  <template>
    <action workflow="deploy-staging" inputs='{"app": "<star/>"}'/>
    Deploying <star/> to staging...
  </template>
</category>

<category>
  <pattern>ROLLBACK *</pattern>
  <template>
    <confirm>
      Rollback <star/> to previous version?
    </confirm>
    <action workflow="rollback-production" inputs='{"app": "<star/>"}'/>
  </template>
</category>

<!-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê -->
<!-- BULK OPERATIONS (GOD MODE) -->
<!-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê -->

<category>
  <pattern>SETUP * ORGS WITH * REPOS EACH</pattern>
  <template>
    <think>
      <set name="org_count"><star index="1"/></set>
      <set name="repo_count"><star index="2"/></set>
    </think>
    <confirm>
      üöÄ BULK GITHUB SETUP
      
      This will create:
      ‚Ä¢ <star index="1"/> organizations
      ‚Ä¢ <star index="2"/> repositories per organization
      
      Total: <compute><star index="1"/> * <star index="2"/></compute> repositories
      
      Proceed?
    </confirm>
    <action workflow="github-bulk-setup" inputs='{
      "orgs": "<star index="1"/>",
      "repos_per_org": "<star index="2"/>"
    }'/>
  </template>
</category>

